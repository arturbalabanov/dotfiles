{
    "name": "friendly-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": ["all"],
                "path": "./global.json"
            },
            {
                "language": ["python"],
                "path": "./python.json"
            },
            {
                "language": ["go"],
                "path": "./go.json"
            },
            {
                "language": ["ansible"],
                "path": "./ansible.json"
            },
            {
                "language": ["yaml"],
                "path": "./yaml.json"
            }
        ]
    }
}
